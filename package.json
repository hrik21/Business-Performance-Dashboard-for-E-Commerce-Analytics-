{
  "name": "ecommerce-bi-dashboard",
  "version": "1.0.0",
  "description": "Interactive real-time business intelligence dashboards for e-commerce platform",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "concurrently \"npm run dev -w @ecommerce-bi/backend\" \"npm run dev -w @ecommerce-bi/frontend\"",
    "start": "npm run start --workspaces",
    "test": "npm run test --workspaces",
    "test:watch": "npm run test:watch --workspaces",
    "lint": "npm run lint --workspaces",
    "lint:fix": "npm run lint:fix --workspaces",
    "migrate": "npm run migrate -w @ecommerce-bi/backend",
    "migrate:rollback": "npm run migrate:rollback -w @ecommerce-bi/backend",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "setup": "npm install && npm run docker:dev"
  },
  "keywords": [
    "business-intelligence",
    "dashboard",
    "analytics",
    "ecommerce",
    "real-time",
    "powerbi"
  ],
  "author": "Development Team",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  }
}